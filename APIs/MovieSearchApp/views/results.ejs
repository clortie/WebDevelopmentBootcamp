<h1>Results:</h1>

<% if(data.Response=="True"){ %>
    <ul>
        <% data.Search.forEach(function(movie){ %>
            <li> <strong><%= movie.Title %></strong> - <%= movie.Year %> </li>
            <% if(movie.Poster!="N/A"){ %>
                <img src=<%=movie.Poster%>>
            <% } %>
        <% }); %>
    </ul>
<% } else {%>
    <p>Search Results Empty!</p>
<% } %>

<a href="/">Search Again</a>